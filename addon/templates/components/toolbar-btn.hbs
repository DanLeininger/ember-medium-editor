{{#me-extension-child
  register=(action _scheduleRegister)
  hasBlock=(has-block)
  options=(hash
    builtIn=builtIn
    name=name
    btnAction=btnAction
    aria=aria
    tagNames=tagNames
    style=style
    useQueryState=useQueryState
    contentDefault=contentFA
    contentFA=contentFA
  ) as |child|}}
  {{#if (has-block)}}
    {{#render-to-string
      afterRender=(action child.addOption "contentDefault")~}}
      {{yield}}
    {{~/render-to-string}}
  {{/if}}
{{/me-extension-child}}
